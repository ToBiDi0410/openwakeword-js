{
  "name": "openwakeword-js",
  "version": "0.0.2",
  "source": "src/index.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts"
    },
    "./models/*": "./dist/models/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf -rf dist",
    "build:npm": "tsc -p tsconfig.json && tsc -p tsconfig.cjs.json",
    "build:browser": "webpack --mode production",
    "build": "npm run clean && npm run build:npm && npm run build:browser",
    "watch": "tsc -w"
  },
  "peerDependencies": {
    "onnxruntime-web": "^1.14.0"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^11.0.0",
    "rimraf": "^6.1.2",
    "ts-loader": "^9.4.0",
    "typescript": "^5.0.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0"
  }
}
