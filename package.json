{
  "name": "openwakeword-js",
  "author": "ToDi0410",
  "version": "0.0.1",
  "source": "src/WakeWordEngine.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "parcel build",
    "watch": "parcel watch"
  },
  "targets": {
    "main": {
      "context": "node",
      "outputFormat": "commonjs",
      "includeNodeModules": false
    },
    "module": {
      "context": "node",
      "outputFormat": "esmodule",
      "includeNodeModules": false
    },
    "web": {
      "source": "src/browser.ts",
      "context": "browser",
      "outputFormat": "global",
      "includeNodeModules": true,
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      }
    },
    "types": {
      "outputFormat": "commonjs"
    }
  },
  "peerDependencies": {
    "onnxruntime-web": "^1.14.0"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.16.3",
    "@parcel/transformer-typescript-types": "^2.16.3",
    "parcel": "^2.9.3",
    "parcel-reporter-static-files-copy": "^1.5.3",
    "typescript": "^5.0.0"
  },
  "staticFiles": [
    {
      "staticPath": "node_modules/onnxruntime-web/dist",
      "staticOutPath": ""
    },
    {
      "staticPath": "models/",
      "staticOutPath": ""
    }
  ]
}
